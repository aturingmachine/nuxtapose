{
  "name": "nuxtapose",
  "version": "0.0.1-beta.0.1.1",
  "description": "Generate Files For Nuxt Projects",
  "keywords": [
    "nuxt",
    "cli",
    "template",
    "generator"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aturingmachine/nuxtapose"
  },
  "homepage": "https://aturingmachine.github.io/nuxtapose/",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc && node dist/index.js",
    "lint": "eslint --fix --max-warnings 0 'src/**'",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:publish": "./scripts/deploy-docs.sh",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "cli-table": "^0.3.6",
    "commander": "^8.1.0",
    "figlet": "^1.5.0",
    "inquirer": "^8.1.2"
  },
  "devDependencies": {
    "@types/cli-table": "^0.3.0",
    "@types/figlet": "^1.5.4",
    "@types/inquirer": "^7.3.3",
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "prettier": "2.3.1",
    "typescript": "^4.3.5",
    "vuepress": "^1.8.2",
    "vuepress-theme-default-prefers-color-scheme": "^2.0.0"
  }
}